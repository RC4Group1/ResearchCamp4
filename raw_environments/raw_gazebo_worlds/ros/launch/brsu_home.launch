<?xml version="1.0"?>
<launch>

<!-- start gazebo with empty world -->
 <!-- <include file="$(find raw_gazebo_worlds)/ros/launch/empty-world.launch" /> -->
 <node name="gazebo" pkg="gazebo" type="gazebo" args="$(find gazebo_worlds)/worlds/empty.world" respawn="false" output="screen" >
	 <env name="GAZEBO_RESOURCE_PATH" value="$(find youbot_description):$(find gazebo_worlds):$(find gazebo)/gazebo/share/gazebo" />
		<remap from="base_controller/command" to="cmd_vel"/>
		<remap from="scan_front" to="base_scan"/>
                <remap from="/base_odometry/odom" to="/odom" />
		
 </node>

	<!-- <node name="gazebo" pkg="gazebo" type="gazebo" args="$(find gazebo_worlds)/worlds/empty.world" respawn="false" output="screen" />	-->

<!-- <param name="robot_description" command="$(find xacro)/xacro.py '$(find youbot_description)/robots/youbot.urdf.xacro'"/> -->
<!-- send world urdf to param server -->
<param name="arena_description" command="$(find xacro)/xacro.py $(find raw_gazebo_worlds)/common/worlds/$(env ROBOT_ENV).urdf" />


<!-- push world_description to factory and spawn robot in gazebo -->
<!-- <node name="youbot_gazebo_model" pkg="gazebo" type="spawn_model" args="-urdf -param robot_description -model youBot -x 0.0 -y 0.0 -z 0.1" respawn="false" output="screen" /> -->
<!-- <node name="spawn_urdf_world" pkg="gazebo" type="spawn_model" args="-urdf -param world_description -x 0 -y 0 -z 0.01 -R 0.0 -P 0.0 -Y 0 -model urdf_world_model" respawn="false" output="screen" /> -->
<node name="arena_gazebo_model" pkg="gazebo" type="spawn_model" args="-urdf -param arena_description -model arena -x 0.0 -y 0.0 -z 0.2" respawn="false" output="screen" />
</launch>
